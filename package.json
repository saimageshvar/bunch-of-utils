{
  "name": "join-text-with-operator",
  "displayName": "Join Text With Operator",
  "description": "It joins the selected texts with operator (:: by default)",
  "version": "1.1.0",
  "repository": "https://github.com/saimageshvar/join-text-with-operator",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.joinTextWithOperator",
        "title": "Join Text with Operator"
      },
      {
        "command": "extension.propToTemplateLiteral",
        "title": "Prop to Template Literal"
      }
    ],
    "keybindings": [
      {
        "command": "extension.joinTextWithOperator",
        "key": "ctrl+shift+j",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Join Text Settings",
      "properties": {
        "joinTextWithOperator.operator": {
          "type": "string",
          "default": "::",
          "description": "The operator to use when joining selected text"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.94.0",
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "eslint": "^9.11.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
